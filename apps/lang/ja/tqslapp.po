msgstr "ヘルプ"

#: getpassword.cpp:111
msgid "New passphrase:"
msgstr "新しいパスフレーズ:"

#: getpassword.cpp:114
msgid "Enter again to confirm:"
msgstr "確認のためもう一度入力してください："

#: getpassword.cpp:146
msgid "Passphrase entries do not match"
msgstr "パスフレーズ入力が一致しません"

#: getpassword.cpp:157
